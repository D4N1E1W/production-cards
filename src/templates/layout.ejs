<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <style><%- style %></style>
    <style>
      @page { size: <%= pageSize %> portrait; }
    </style>
    <title>Beat Cards</title>
  </head>
  <body>
    <div class="wrapper">
      <% beats.forEach(function(beat, idx) { %>
        <%- renderCard({ beat }) %>
      <% }) %>
    </div>
  </body>
</html>